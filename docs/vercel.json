{
  "builds": [
    {
      "src": "webapp.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "webapp.py"
    },
    {
      "src": "/(.*\\.(html|css|js|json|png|jpg|gif|txt|xml|md))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
