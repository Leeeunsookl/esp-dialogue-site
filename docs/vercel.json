{
  "version": 2,
  "functions": {
    "handler.py": {
      "runtime": "python3.11" 
    }
  },
  "routes": [
    
    {
      "src": "/api/existence/reply",
      "dest": "handler.py"
    },

    {
      "src": "/api/(.*)",
      "dest": "handler.py"
    },
    
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
